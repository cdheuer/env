# Prompt
PS1='\033[01;32m\]\u@\h\[\033[00m\]:\[\033[01;34m\]\w \$\[\033[00m\] '

alias c=clear
alias gs='git status'
alias gb='git branch'
alias gch='git checkout'
# Color output
alias l='ls -G'
alias ..='cd ..'

# Stop all bells
bind 'set bell-style none'

export PATH="/opt/homebrew/bin:$PATH"
export PATH="/opt/homebrew/sbin:$PATH"

